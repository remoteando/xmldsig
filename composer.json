{
    "name": "remoteando/xmldsig",
    "description": "Tool for easy management of XML Signatures (http://www.w3.org/TR/xmldsig-core/)",
    "license": "BSD-2-Clause",
    "keywords": [
        "xml",
        "xmldsig",
        "signature",
        "security",
        "soap"
    ],
    "authors": [
        {
            "name": "Maks3w",
            "email": "github.maks3w@virtualplanets.net"
        }
    ],
    "homepage": "https://github.com/remoteando/xmldsig",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/remoteando/xmlseclibs"
        }
    ],
    "require": {
        "remoteando/xmlseclibs": "^1.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.*",
        "phpunit/phpunit": ">= 3.7"
    },
    "autoload": {
        "psr-4": {
            "Remoteando\\XmlDSig\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Remoteando\\XmlDSigTest\\": ["test"]
        }
    }
}
